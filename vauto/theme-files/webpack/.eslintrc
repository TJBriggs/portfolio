{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended"
	],
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"globals": {
		"lodash": true,
		"$": false,
		"jQuery": false,
		"TimelineLite": false,
		"TimelineMax": false,
		"TweenLite": false,
		"TweenMax": false,
		"Back": false,
		"Bounce": false,
		"Circ": false,
		"Cubic": false,
		"Ease": false,
		"EaseLookup": false,
		"Elastic": false,
		"Expo": false,
		"Linear": false,
		"Power0": false,
		"Power1": false,
		"Power2": false,
		"Power3": false,
		"Power4": false,
		"Quad": false,
		"Quart": false,
		"Quint": false,
		"RoughEase": false,
		"ScrollMagic": false,
		"Sine": false,
		"SlowMo": false,
		"SteppedEase": false,
		"Strong": false,
		"Draggable": false,
		"SplitText": false,
		"VelocityTracker": false,
		"CSSPlugin": false,
		"ThrowPropsPlugin": false,
		"BezierPlugin": false
	},
	"rules": {
		"no-console": 1,
		"prettier/prettier": [
			2,
			{
				"useTabs": true,
				"endOfLine": "auto",
				"tabWidth": 4,
				"printWidth": 100,
				"singleQuote": true,
				"trailingComma": "all",
				"bracketSpacing": true,
				"parenSpacing": false,
				"jsxBracketSameLine": false,
				"semi": true,
				"arrowParens": "always"
			},
			{
				"usePrettierrc": false
			}
		]
	}
}

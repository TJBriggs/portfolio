{
	"$schema": "https://json.schemastore.org/block.json",
	"apiVersion": 2,
	"name": "mx/uberflip-features",
	"version": "0.1.0",
	"title": "Uberflip Features",
	"category": "features",
	"description": "Enter an Uberflip embed ID to display 3 articles.",
	"attributes": {
		"marginBottom": {
			"type": "string",
			"default": "none"
		},
		"marginTop": {
			"type": "string",
			"default": "none"
		},
		"paddingBottom": {
			"type": "string",
			"default": "default"
		},
		"paddingTop": {
			"type": "string",
			"default": "default"
		},
		"widthOption": {
			"type": "string",
			"default": "centered"
		},
		"widthIcon": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"reusable": true,
		"html": false,
		"color": {
			"background": true,
			"gradients": true
		}
	},
	"textdomain": "mx-blocks-plugin",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css"
}
